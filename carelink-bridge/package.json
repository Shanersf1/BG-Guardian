{
  "name": "carelink-bridge",
  "version": "2.0.0",
  "description": "Bridge that sends Medtronic CareLink pump and CGM data to Nightscout by simulating the CareLink mobile app's OAuth2 authentication and API calls.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "tsx src/main.ts",
    "login": "tsx src/login.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\""
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT",
  "keywords": [
    "medtronic",
    "minimed",
    "carelink",
    "cgm",
    "nightscout",
    "diabetes"
  ],
  "dependencies": {
    "axios": "^1.7.0",
    "dotenv": "^16.4.0",
    "https-proxy-agent": "^7.0.0",
    "qs": "^6.13.0",
    "puppeteer-core": "^24.0.0",
    "socks-proxy-agent": "^8.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/qs": "^6.9.0",
    "tsx": "^4.0.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  }
}
